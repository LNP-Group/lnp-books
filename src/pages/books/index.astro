---
import MainLayout from "../../layouts/MainLayout.astro";

const title = "Books";
const description = "Welcome to the official website of LNP Books.";
const url = "https://www.google.com";
const locale = "en-US";
const image = "https://pixlr.com/images/index/remove-bg.webp";

// CSS
import '../../styles/global.css';
import '../../styles/books.css';


---

<MainLayout
	title={title}
	description={description}
	url={url}
	locale={locale}
	image={image}
>
<div class="books">
	<div class="header">
		<h1>Books</h1>
	</div>
	<div id="main" class="main">
	</div>
</div>
</MainLayout>